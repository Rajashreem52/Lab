node{
    checkout scm
    stage('Build') {
       withMaven(maven: 'maven') {
                    sh 'mvn compile'
                }
        git 'https://github.com/Rajashreem52/Lab/blob/master/JenkinsFile'
    }
    stage('Test') {
    
       withMaven(maven: 'maven') {
                    sh 'mvn test'
                }
    }
}
