node{
    checkout scm
    stage('Build') {
       withMaven(maven: 'maven') {
                    sh 'mvn compile'
                }
        git 'http://github.com/brentlaster/roarv2'
    }
    stage('Test') {
    
       withMaven(maven: 'maven') {
                    sh 'mvn test'
                }
    }
}
